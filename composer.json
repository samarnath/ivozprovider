{
    "name": "irontec/ivozprovider",
    "description": "Multitenant solution for VoIP telephony providers",
    "authors": [
        {
            "name": "Carlos Cruz",
            "email": "carlos@irontec.com"
        },
        {
            "name": "Gaizka Elexpe",
            "email": "gaizka@irontec.com"
        },
        {
            "name": "Gorka Gorrotxategi",
            "email": "gorka.g@irontec.com"
        },
        {
            "name": "Ivan Alonso",
            "email": "kaian@irontec.com"
        },
        {
            "name": "Luis Felipe GarcÃ­a",
            "email": "lfgarcia@irontec.com"
        },
        {
            "name": "Mikel Madariaga",
            "email": "mikel@irontec.com"
        },
        {
            "name": "Victor Vargas",
            "email": "victor@irontec.com"
        }
    ],
    "repositories": [
        {
            "type":"git",
            "url": "https://github.com/irontec/klear-generator"
        },
        {
            "type":"git",
            "url": "https://github.com/irontec/klear-library.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "": "symfony/src/"
        },
        "classmap": [
            "symfony/kernel/AppKernel.php",
            "symfony/kernel/ZfKernel.php",
            "symfony/kernel/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "symfony/tests/"
        },
        "files": [
            "library/vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "config": {
        "vendor-dir": "library/vendor",
        "sort-packages": true,
        "preferred-install": {
            "*": "git"
        }
    },
    "require": {
        "php": ">=5.6.30",
        "symfony/symfony": "3.2.*",
        "sensio/distribution-bundle": "^5.0",
        "symfony/monolog-bundle": "^3.0.2",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "beberlei/assert": "2.7.*",

        "phing/phing": "2.*",
        "knplabs/knp-snappy": "0.4.3",
        "h4cc/wkhtmltopdf-amd64": "0.12.3",
        "h4cc/wkhtmltopdf-i386": "0.12.3",
        "crada/php-apidoc":"@dev",
        "xamin/handlebars.php": "^0.10.4",
        "debach/zend-mp3": "1.8.*"
    },
    "require-dev": {
        "crada/php-apidoc":"@dev",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^3.0",
        "irontec/Generator": "dev-tayler",
        "irontec/Iron": "dev-tayler"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "extra": {
        "symfony-app-dir": "symfony/app",
        "symfony-bin-dir": "symfony/bin",
        "symfony-var-dir": "symfony/var",
        "symfony-web-dir": "symfony/web",
        "symfony-tests-dir": "symfony/tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "symfony/kernel/config/parameters.yml"
        },
        "branch-alias": null
    }
}
